def prec_prom(ventas):
    sum_prec = {}
    cont_prod = {}

    for venta in ventas:
        prod = venta["producto"]
        prec = venta["precio"]

        if prod in sum_prec:
            sum_prec[prod] += prec
            cont_prod[prod] += 1
        else:
            sum_prec[prod] = prec
            cont_prod[prod] = 1

    prom_prec = {prod: sum_prec[prod] / cont_prod[prod] for prod in sum_prec}
    return prom_prec

