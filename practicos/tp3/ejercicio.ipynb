def cargar_ventas():
    lineas = open("datos.dat", "r").readlines()
    ventas = []

    for linea in lineas:
        venta = {
            "fecha": linea[0:10].strip(),
            "producto": linea[10:40].strip(),
            "precio": float(linea[40:50].strip()),
            "cantidad": int(linea[50:55].strip())
        }
        ventas.append(venta)

    return ventas

lista_ventas = cargar_ventas()
