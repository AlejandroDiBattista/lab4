def vent_mes(ventas):
    mes_prod = {}

    for venta in ventas:
        fecha = venta["fecha"]
        mes = fecha[5:7]  
        prod = venta["producto"]
        cant = venta["cantidad"]

        if mes not in mes_prod:
            mes_prod[mes] = {}

        if prod in mes_prod[mes]:
            mes_prod[mes][prod] += cant
        else:
            mes_prod[mes][prod] = cant

    return mes_prod

